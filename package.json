  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@types/prismjs": "^1.16.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.19.0",
    "cross-env": "^5.2.1",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^3.14.0",
    "laravel-mix": "^4.1.4",
    "php-parser": "^3.0.2",
